{
    "name": "jamarcer/symfony-messenger-transaction",
    "description": "Symfony Messenger Middleware for Transactional Database connections",
    "version": "1.0.0",
    "license": "MIT",
    "type": "library",
    "authors": [
    {
        "name": "Jesús Martínez",
        "email": "jamarcer@gmail.com"
    }
    ],
    "autoload": {
        "psr-4": {
            "Jamarcer\\Transaction\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Jamarcer\\Tests\\Transaction\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/messenger": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "dg/bypass-finals": "^1.3"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
